    "pathfinding": "^0.4.17",
    "uuid": "^2.0.1",
    "ws": "^6.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "madge": "^3.3.0",
    "mocha": "^5.2.0",
    "webpack": "^4.25",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
